{
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": ".vercel/output/static",
  "installCommand": "npm ci",
  "framework": "sveltejs",
  "functions": {
    "api/server.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/gallery-portfolio/$1"
    }
  ]
} 