{
  "buildCommand": "cd gallery-portfolio && npm ci && npm run build",
  "outputDirectory": "gallery-portfolio/.vercel/output/static",
  "installCommand": "cd gallery-portfolio && npm ci",
  "framework": null,
  "functions": {
    "api/server.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/gallery-portfolio/$1"
    }
  ]
} 